{
  "name": "output-build-info-plugin",
  "version": "1.0.2",
  "description": "After the project is packaged with an output version informationã€‚",
  "main": "./dist/app.js",
  "scripts": {
    "lint": "eslint --ext .js ./",
    "release": "node scripts/release.js",
    "build": "npx webpack --progress --config ./build/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NuoHui/output-build-info-plugin.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "plugin",
    "webpack",
    "git"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "author": "NuoHui",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NuoHui/output-build-info-plugin/issues"
  },
  "homepage": "https://github.com/NuoHui/output-build-info-plugin#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@babel/preset-env": "^7.7.7",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.1",
    "standard-version": "^8.0.1",
    "terser-webpack-plugin": "^3.0.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "semver": "^6.1.1",
    "@babel/core": "^7.7.7"
  },
  "dependencies": {
    "simple-git": "^3.3.0"
  }
}
